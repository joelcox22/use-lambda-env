{
  "tasks": {
    "build": {
      "name": "build",
      "steps": [
        {
          "exec": "tsc"
        },
        {
          "exec": "rm -rf lib/*.test.js lib/*.test.d.ts"
        },
        {
          "exec": "chmod +x lib/cli.js"
        }
      ]
    },
    "lint": {
      "name": "lint",
      "steps": [
        {
          "exec": "eslint ."
        }
      ]
    },
    "lint:fix": {
      "name": "lint:fix",
      "steps": [
        {
          "exec": "eslint . --fix"
        }
      ]
    },
    "test": {
      "name": "test",
      "steps": [
        {
          "exec": "jest --coverage --passWithNoTests"
        }
      ]
    },
    "update-boilerplate": {
      "name": "update-boilerplate",
      "steps": [
        {
          "exec": "yarn add @joelcox22/boilerplate@latest -E"
        },
        {
          "exec": "projen"
        },
        {
          "exec": "jest -u --passWithNoTests"
        },
        {
          "exec": "yarn build"
        }
      ]
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
